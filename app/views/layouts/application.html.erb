<!DOCTYPE html>
<html>
  <head>
    <title>
      <% if Setting.exists? %>
        <%= Setting.first.logostring %>
      <% else %>
        Empty
      <% end %>
    </title>
    <%= stylesheet_link_tag    "application","coderay", media: "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <!--[if lt IE 9]>
    <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
    <![endif]-->
		<%= render 'layouts/shim' %>    

  </head>
  <body>
    <%= render 'layouts/header' %>
    <div class="container">
      <%= yield %>
    </div>
  </body>
</html>
